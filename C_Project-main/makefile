ABC.exe:main.o big2.o big3.o fact.o rev.o  palindrome.o sum2num.o sort.o fib.o
	gcc -o ABC.exe main.o big2.o big3.o fact.o rev.o palindrome.o sum2num.o sort.o fib.o
main.o:main.c
	gcc -c main.c
big2.o:big2.c
	gcc -c big2.c
big3.o:big3.c
	gcc -c big3.c
fact.o:fact.c
	gcc -c fact.c
rev.o:rev.c
	gcc -c rev.c
palindrome.o:palindrome.c
	gcc -c palindrome.c
sum2num.o:sum2num.c
	gcc -c sum2num.c
sort.o:sort.c
	gcc -c sort.c
fib.o:fib.c
	gcc -c fib.c
clean:
	rm -rf *.o
